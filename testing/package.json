{
  "name": "testing",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run front\" \"npm run back\"",
    "front": "ng serve  --proxy-config ./proxy.json",
    "back": "ts-node -P ./server/server.tsconfig.json ./server/server.ts",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "build:prod": "ng build --configuration production",
    "start:prod": "http-server ./dist/testing/browser -p 4200",
    "generate:reports": "ng test --watch=false --code-coverage",
    "serve:reports": "http-server ./coverage/testing -c-1 -p 4201",
    "build:start:prod": "run-s build:prod start:prod",
    "e2e": "start-server-and-test build:start:prod http://localhost:4200 cypress:run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.0",
    "@angular/cdk": "^18.2.0",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/material": "^18.2.0",
    "@angular/material-moment-adapter": "^18.2.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/router": "^18.1.0",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "http-server": "^14.1.1",
    "moment": "^2.30.1",
    "rxjs": "~7.8.0",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.4",
    "@angular/cli": "^18.1.4",
    "@angular/compiler-cli": "^18.1.0",
    "@types/jasmine": "~5.1.0",
    "cypress": "^13.13.3",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^2.0.5",
    "typescript": "~5.5.2"
  }
}
